<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:c="urn:import:com.google.gwt.user.cellview.client">
	<ui:style>
		.important {
			font-weight: bold;
		}
	</ui:style>	
	<g:HTMLPanel ui:field="htmlPanel">		
		<table>
			<tr>
				<td valign='top'>
					This is template for cell tree view
				</td>
			</tr>
			<tr>
				<td align='center'>					
						<g:HorizontalPanel>
							<g:VerticalPanel>
								<c:CellTree ui:field="cellTree1" />
							</g:VerticalPanel>
							<g:VerticalPanel>
								<c:CellTree ui:field="cellTree2" />
							</g:VerticalPanel>
						</g:HorizontalPanel>					
				</td>
			</tr>
		</table>
	</g:HTMLPanel>	
</ui:UiBinder> 